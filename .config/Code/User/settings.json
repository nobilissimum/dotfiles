{
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.formatOnPaste": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never",
      "source.organizeImports": "never"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.detectIndentation": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.wordBasedSuggestions": "off",
    "gitlens.codeLens.symbolScopes": ["!Module"]
  },
  "[shellscript, sh]": {
    "editor.tabSize": 4,
    "files.eol": "\n"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.icons": true,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showStructs": false,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showVariables": false,
  "chat.editor.fontFamily": "default",
  "code-eol.crlfCharacter": "",
  "code-eol.newlineCharacter": "↓",
  "code-eol.returnCharacter": "",
  "codesnap.backgroundColor": "#00000000",
  "codesnap.containerPadding": "0em",
  "codesnap.realLineNumbers": true,
  "codesnap.showLineNumbers": true,
  "codesnap.showWindowTitle": false,
  "codesnap.transparentBackground": true,
  "color-highlight.enable": true,
  "color-highlight.markRuler": false,
  "color-highlight.markerType": "underline",
  "color-highlight.matchWords": true,
  "debug.console.fontFamily": "'Source Code Pro', 'SauceCodePro Nerd Font', 'Fira Code', 'FiraCode Nerd', 'Roboto Mono', 'RobotoMono NF', Inconsolata, Consolas, 'Courier New', monospace",
  "debug.console.fontSize": 14,
  "debug.console.wordWrap": true,
  "diffEditor.wordWrap": "on",
  "dotenv.enableAutocloaking": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.codeLens": true,
  "editor.codeLensFontFamily": "'Fira Sans', Inter, Roboto, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "editor.colorDecorators": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.cursorStyle": "line-thin",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Source Code Pro', 'SauceCodePro Nerd Font', 'Fira Code', 'FiraCode Nerd', 'Roboto Mono', 'RobotoMono NF', Inconsolata, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.fontWeight": "normal",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.inlayHints.fontFamily": "'Fira Sans', Inter, Roboto, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "editor.letterSpacing": 0,
  "editor.lineHeight": 1.6,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 2,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.minimap.size": "proportional",
  "editor.renderWhitespace": "all",
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLineCount": 10,
  "editor.stickyScroll.scrollWithEditor": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.tokenColorCustomizations": {
    "textMateRules": []
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "›": true
  },
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 88,
  "errorLens.delay": 0,
  "errorLens.fontFamily": "'Source Code Pro', 'SauceCodePro Nerd Font', 'Fira Code', 'FiraCode Nerd', 'Roboto Mono', 'RobotoMono NF', Inconsolata, Consolas, 'Courier New', monospace",
  "errorLens.enabled": true,
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info"],
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.validate": ["javascript", "jsx", "typescript", "tsx"],
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "files.associations": {
    ".env*": "dotenv",
    "*.inc": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "functional": "cpp",
    "array": "cpp",
    "*.tcc": "cpp",
    "memory": "cpp",
    "future": "cpp",
    "istream": "cpp",
    "tuple": "cpp",
    "utility": "cpp",
    "variant": "cpp",
    "compare": "cpp",
    "thread": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "span": "cpp",
    "vector": "cpp"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false
  },
  "gitlens.blame.avatars": true,
  "gitlens.blame.compact": false,
  "gitlens.codeLens.enabled": false,
  "indentRainbow.colors": ["rgba(255,255,255,0.03)", "rgba(255,255,255,0.00)"],
  "indentRainbow.errorColor": "rgba(128,32,32,0.5)",
  "indentRainbow.tabmixColor": "rgba(128,32,96,0.5)",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Fira Sans', Inter, Roboto, sans-serif, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "notebook.breadcrumbs.showCodeCells": false,
  "notebook.output.fontFamily": "'Source Code Pro', 'SauceCodePro Nerd Font', 'Fira Code', 'FiraCode Nerd', 'Roboto Mono', 'RobotoMono NF', Inconsolata, Consolas, 'Courier New', monospace",
  "notebook.output.textLineLimit": 1000,
  "prettier.endOfLine": "lf",
  "prettier.jsxSingleQuote": true,
  "prettier.quoteProps": "as-needed",
  "prettier.singleQuote": true,
  "prettier.tabWidth": 4,
  "prettier.trailingComma": "all",
  "prisma.showPrismaDataPlatformNotification": false,
  "python.analysis.addImport.exactMatchOnly": false,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.indexing": true,
  "python.analysis.persistAllIndices": true,
  "python.diagnostics.sourceMapsEnabled": true,
  "python.languageServer": "Pylance",
  "remote.autoForwardPortsSource": "hybrid",
  "scm.inputFontFamily": "default",
  "scss.validate": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
  "terminal.integrated.fontFamily": "'Source Code Pro', 'SauceCodePro Nerd Font', 'Fira Code', 'FiraCode Nerd', 'Roboto Mono', 'RobotoMono NF', Inconsolata, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.letterSpacing": 0,
  "terminal.integrated.lineHeight": 1.25,
  "terminal.integrated.minimumContrastRatio": 1,
  "trailing-spaces.backgroundColor": "rgba(239,89,89,0.5)",
  "trailing-spaces.borderColor": "rgba(239,89,89,0.00)",
  "typescript.preferences.quoteStyle": "single",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vsintellicode.typescript.completionsEnabled": true,
  "window.commandCenter": false,
  "window.title": "${activeRepositoryName}",
  "window.titleBarStyle": "custom",
  "window.titleSeparator": " • ",
  "workbench.colorCustomizations": {},
  "workbench.colorTheme": "Hush",
  "workbench.editor.centeredLayoutAutoResize": true,
  "workbench.editor.centeredLayoutFixedWidth": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zenMode.centerLayout": true
}
